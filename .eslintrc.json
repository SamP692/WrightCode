{
    "extends": "airbnb",
    "installedESLint": true,
    "plugins": [
        "react",
        "jsx-a11y",
        "import"
    ],
    "env": {
      "browser": true,
      "node": true
    },
    "rules": {
      "react/jsx-filename-extension": [0],
      "no-multi-spaces": [0],
      "react/prefer-stateless-function": [1],
      "no-unused-vars": [1],
      "react/prop-types": [0],
      "max-len": [1],
      "react/jsx-indent": [1],
      "no-trailing-spaces": [1],
      "no-useless-constructor": [1]
    }
}
